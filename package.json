{
  "name": "azur-lane-parser",
  "version": "0.0.1",
  "private": true,
  "description": "Standalone patch notes parser for Azur Lane - fetches and processes official patch notes",
  "type": "module",
  "module": "main.ts",
  "scripts": {
    "main": "bun main.ts",
    "scrape": "bun main.ts -s && bun check",
    "process": "bun main.ts -p && bun check",
    "opencode-parse": "bun main.ts -o && bun check",
    "check": "oxlint --fix . && oxfmt .",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.30",
    "ai": "^6.0.99",
    "cheerio": "^1.2.0",
    "turndown": "^7.2.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/node": "^25.3.0",
    "@types/turndown": "^5.0.6",
    "lefthook": "^2.1.1",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
